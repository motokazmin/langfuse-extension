# ====================================================================
# AI ПРОВАЙДЕР - выберите один из: openrouter, ollama
# ====================================================================
AI_PROVIDER=ollama
# AI_PROVIDER=openrouter

# ====================================================================
# НАСТРОЙКИ ДЛЯ OPENROUTER (если AI_PROVIDER=openrouter)
# ====================================================================
AI_API_KEY=your-openrouter-api-key-here
AI_BASE_URL=https://openrouter.ai/api/v1
AI_MODEL=google/gemini-2.0-flash-exp:free
# Другие модели OpenRouter:
# AI_MODEL=anthropic/claude-3.5-sonnet
# AI_MODEL=meta-llama/llama-3.2-90b-vision-instruct
# AI_MODEL=openai/gpt-4-turbo

# ====================================================================
# НАСТРОЙКИ ДЛЯ OLLAMA (если AI_PROVIDER=ollama)
# ====================================================================
OLLAMA_BASE_URL=http://localhost:11434
OLLAMA_MODEL=llama3.2
OLLAMA_TIMEOUT=300
# Другие модели Ollama (убедитесь, что они загружены):
# OLLAMA_MODEL=mistral
# OLLAMA_MODEL=codellama
# OLLAMA_MODEL=llama2
# OLLAMA_MODEL=phi
# OLLAMA_MODEL=gemma

# ====================================================================
# ОБЩИЕ НАСТРОЙКИ AI
# ====================================================================
AI_MAX_TOKENS=1000

# ====================================================================
# LANGFUSE НАСТРОЙКИ
# ====================================================================
LANGFUSE_PUBLIC_KEY=your-langfuse-public-key
LANGFUSE_SECRET_KEY=your-langfuse-secret-key
LANGFUSE_BASEURL=https://cloud.langfuse.com

# ====================================================================
# CHROME EXTENSION
# ====================================================================
CHROME_EXTENSION_ID=your-chrome-extension-id
